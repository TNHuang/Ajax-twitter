<% _(users).each(function (user) { %>
  <li>
    <a href="<%= user_url %>"><%= user.username %></a>
    <button class='follow-toggle'
            data-user-id="<%= user.id %>"
            data-initial-follow-state=<% current_user.follows?(user) ? "followed" : "unfollowed" %>></button>
  </li>
<% }); %>